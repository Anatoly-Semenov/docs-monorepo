{
	"name": "docs-monorepo",
	"version": "1.0.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest --config test/config/jest.config.ts",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config test/config/jest-e2e.config.ts",
		"typeorm": "typeorm-ts-node-commonjs",
		"migration:generate": "yarn typeorm migration:generate --dataSource dist/shared/config/typeorm.config.js",
		"migration:run": "yarn typeorm migration:run --dataSource dist/shared/config/typeorm.config.js",
		"migration:revert": "yarn typeorm migration:revert --dataSource dist/shared/config/typeorm.config.js",
		"seed:create": "yarn ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create --name src/common/db/seeds/organizations.seeder.ts",
		"seed:run": "yarn ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d dist/shared/config/typeorm.config.js",
		"seed:revert": "yarn ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:revert -d dist/shared/config/typeorm.config.js",
		"prepare": "husky"
	},
	"pre-commit": [
		"prepare"
	],
